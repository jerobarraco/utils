#!/bin/bash
for number in {1..2}
do
  echo Running client "$number"
  #/Users/user/dist/client.py sleep 2 &
  ./client.py sleep 2 &
done

wait < <(jobs -p)
exit 0


for job in `jobs -p`
do
	echo $job
	wait $job 
#|| let "FAIL+=1"
done

echo $FAIL

if [ "$FAIL" == "0" ];
then
echo "YAY!"
else
echo "FAIL! ($FAIL)"
fi
